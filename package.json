{
  "name": "fourzainsider-api",
  "version": "1.0.0",
  "description": "API Fourza Insider",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --dir src/routes/controllers",
    "test:e2e:watch": "vitest --dir src/routes/controllers",
    "test:coverage": "vitest run --coverage",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "vercel-build": "npx prisma migrate deploy",
    "build": "tsup src --out-dir build",
    "postinstall": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathandpalatin/fourzainsider-api.git"
  },
  "keywords": [],
  "author": "Nathan Palatin",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "9.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "fastify": "^4.26.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "prisma": "5.21.1",
    "supertest": "^6.3.4",
    "tsup": "7.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite-plugin-tsconfig-paths": "^1.4.1",
    "vitest": "^2.1.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.651.1",
    "@aws-sdk/s3-request-presigner": "^3.651.1",
    "@fastify/cookie": "^9.3.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "@prisma/client": "5.21.1",
    "@types/socket.io": "3.0.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "fastify-type-provider-zod": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "socket.io": "4.8.1",
    "zod": "^3.22.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}