{
  "name": "vance-api",
  "version": "1.0.0",
  "description": "API Vance",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --dir src/routes/controllers",
    "test:e2e:watch": "vitest --dir src/routes/controllers",
    "test:coverage": "vitest run --coverage",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "vercel-build": "npx prisma migrate deploy",
    "build": "tsup src --out-dir build",
    "postinstall": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathandpalatin/vance-api.git"
  },
  "keywords": [],
  "author": "Nathan Palatin",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "9.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.9",
    "@types/nodemailer": "6.4.16",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "fastify": "5.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "prisma": "6.0.1",
    "supertest": "^6.3.4",
    "tsup": "7.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite-plugin-tsconfig-paths": "^1.4.1",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@fastify/cookie": "11.0.1",
    "@fastify/cors": "10.0.1",
    "@fastify/jwt": "9.0.1",
    "@fastify/multipart": "9.0.1",
    "@prisma/client": "6.0.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "fastify-type-provider-zod": "4.0.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "6.9.16",
    "pg": "^8.13.1",
    "socket.io": "4.8.1",
    "zod": "^3.23.8"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
